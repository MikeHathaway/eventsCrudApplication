<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <table class="table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Capcity</th>
        <th>Address</th>
        <th>City</th>
        <th>State</th>
        <th>See Events Here</th>
        <th>Edit</th>
      </tr>
    </thead>
    <tbody>
      {{#each venue}}
      <tr>
        <td>{{this.venue_name}}</td>
        <td>{{this.capacity}}</td>
        <td>{{this.line_1}}</td>
        <td>{{this.city}}</td>
        <td>{{this.state}}</td>
        <td><a href="/venues/{{this.id}}"><p>See Events Here</p></a></td>
        <td><a href="#"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></a></td>
      </tr>
      {{/each}}
    </tbody>
    </table>

  </body>
</html>
