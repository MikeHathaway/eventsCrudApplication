<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <table class="table">
    <thead>
      <tr>
        <th>Event</th>
        <th>Description</th>
        <th>Venue</th>
        <th>Starting</th>
        <th>Ending</th>
        <th>Over 21</th>
        <th>Edit</th>
        <th>Register</th>
      </tr>
    </thead>
    <tbody>
      {{#each events}}
      <tr>
        <td><a href="events/{{this.id}}">{{this.title}}</a></td>
        <td>{{this.description}}</td>
        <td><a href="../venues/{{this.venues_id}}">{{this.venues_id}}</a></td>
        <td>{{this.start_datetime}}</td>
        <td>{{this.end_datetime}}</td>
        <td>{{this.over_21}}</td>
        <td><a href="#"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></a></td>
        <td><a href="events/{{this.id}}/register"><p>Register For Event</p></a>
      </tr>
      {{/each}}
    </tbody>
  </table>
  </body>
</html>
