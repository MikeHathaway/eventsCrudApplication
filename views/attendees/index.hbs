<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <table class="table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Birthday</th>
        <th>Edit Info</th>
      </tr>
    </thead>
    <tbody>
      {{#each attendee}}
      <tr>
        <td><a href="../../attendees/{{this.id}}">{{this.preferred_name}}</a></td>
        <td>{{this.email}}</td>
        <td>{{this.birthday}}</td>
        <td><a href="#"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></a></td>
      </tr>
      {{/each}}
    </tbody>
    </table>
  </body>
</html>
